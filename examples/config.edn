{:log-level :debug
 :docker-exposure #{:websocket :rest :mqtt}
 :docker {:api-version "v1.40"
          :hosts {:local {:uri "///var/run/docker.sock"
                          :events true}
                  :remote {:uri "tcp://192.168.0.1:2375"
                           :events false
                           :username "user"
                           :password "passwd"}}}
 :web {:port 3000}
 :websocket {:refresh 300}
 :mqtt {:uri "tcp://192.168.0.2:1883"
        :refresh 300
        :username "user"
        :password "pass"
        :platforms #{:home-assistant}}
 :rest {:username "user"
        :pass "pass"}}
